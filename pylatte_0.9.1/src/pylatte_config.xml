<?xml version="1.0"?>


<config>
	<pylatte-server>
		<port>8000</port>
	</pylatte-server>

	<latteUrl>
		<pylatte>
			<pylatte-name>default</pylatte-name>
			<pylatte-pyl>index.pyl</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>default</pylatte-name>
			<url-mapping>/</url-mapping>
		</pylatte-mapping>	
		
		<pylatte>
			<pylatte-name>index</pylatte-name>
			<pylatte-pyl>index.pyl</pylatte-pyl>
		</pylatte>
		<pylatte-mapping>
			<pylatte-name>index</pylatte-name>
			<url-mapping>/index</url-mapping>
		</pylatte-mapping>	

	</latteUrl>
	
	<latteFilter>
	
		<filter>
			<filter-name>head</filter-name>
			<filter-pyl>filter/filter.pyl</filter-pyl>
		</filter>
		<filter-mapping>
			<filter-name>head</filter-name>
			<filter-url>index.pyl</filter-url>
		</filter-mapping>
		
	</latteFilter>
	
</config>


